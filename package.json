{
  "name": "react-template",
  "version": "0.0.1",
  "description": "react template, hot-reload, webpack-2",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config ./webpack.prod.config.js -p",
    "lint:src": "eslint src/** --config ./.eslintrc_prod",
    "lint:test": "eslint test/** --config ./.eslintrc_prod",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js 'test/**/*.js'",
    "test:watch": "npm run test -- --watch",
    "validate": "npm-run-all --parallel lint:* test"
  },
  "pre-commit": [
    "precommit-msg",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istok20/WE-Cloud.git"
  },
  "author": "Maxim P.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LINK"
  },
  "homepage": "https://github.com/USER_NAME/REPO_NAME#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.9.0",
    "invariant": "^2.2.2",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mock-local-storage": "^1.0.2",
    "node-sass": "^4.1.1",
    "npm-run-all": "^3.1.2",
    "postcss-loader": "^1.2.1",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "react-router-redux": "^4.0.7",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "sizzle": "^2.3.3",
