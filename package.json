    "url-loader": "^0.5.7",
    "webpack": "maxfarseer/webpack#master",
    "webpack-dev-server": "^2.2.0",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "glob": "^7.1.1",
    "isomorphic-fetch": "^2.2.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  }
}
